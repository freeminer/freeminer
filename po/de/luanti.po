msgstr "Nebelbeginn"

#: src/settings_translation_file.cpp
msgid "Font"
msgstr "Schrift"

#: src/settings_translation_file.cpp
msgid "Font bold by default"
msgstr "Schrift standardmäßig fett"

#: src/settings_translation_file.cpp
msgid "Font italic by default"
msgstr "Schrift standardmäßig kursiv"

